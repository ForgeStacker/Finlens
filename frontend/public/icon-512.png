<svg width="512" height="512" viewBox="0 0 512 512" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle with gradient -->
  <defs>
    <radialGradient id="bg-gradient" cx="50%" cy="30%" r="70%">
      <stop offset="0%" style="stop-color:#FFB84D;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF9900;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="lens-gradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#60A5FA;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3B82F6;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <circle cx="256" cy="256" r="256" fill="url(#bg-gradient)"/>
  
  <!-- Eye shape for FinLens -->
  <ellipse cx="256" cy="256" rx="192" ry="128" fill="#1a1b23" stroke="#ffffff" stroke-width="8"/>
  
  <!-- Inner eye/lens -->
  <circle cx="256" cy="256" r="75" fill="url(#lens-gradient)"/>
  
  <!-- Inner pupil/focus point -->
  <circle cx="256" cy="256" r="32" fill="#ffffff"/>
  
  <!-- Lens reflection -->
  <ellipse cx="224" cy="224" rx="20" ry="30" fill="#ffffff" opacity="0.7"/>
  
  <!-- Tech scanner elements -->
  <circle cx="256" cy="256" r="110" fill="none" stroke="#ffffff" stroke-width="2" opacity="0.3" stroke-dasharray="10 10"/>
  <circle cx="256" cy="256" r="140" fill="none" stroke="#ffffff" stroke-width="1" opacity="0.2" stroke-dasharray="5 15"/>
  
  <!-- Corner tech elements -->
  <g opacity="0.4">
    <rect x="100" y="100" width="20" height="4" fill="#ffffff" rx="2"/>
    <rect x="100" y="108" width="12" height="4" fill="#ffffff" rx="2"/>
    <rect x="392" y="100" width="20" height="4" fill="#ffffff" rx="2"/>
    <rect x="400" y="108" width="12" height="4" fill="#ffffff" rx="2"/>
    <rect x="100" y="400" width="20" height="4" fill="#ffffff" rx="2"/>
    <rect x="100" y="408" width="12" height="4" fill="#ffffff" rx="2"/>
    <rect x="392" y="400" width="20" height="4" fill="#ffffff" rx="2"/>
    <rect x="400" y="408" width="12" height="4" fill="#ffffff" rx="2"/>
  </g>
</svg>